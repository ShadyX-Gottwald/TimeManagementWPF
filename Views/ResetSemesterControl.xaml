<UserControl x:Class="WPF_TIME_MANAGEMENT.Views.ResetSemesterControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WPF_TIME_MANAGEMENT.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" Width="737">
    <Grid Margin="0,0,112,35">

        <Calendar Name="cldSample" SelectionMode="MultipleRange" Margin="0,69,38,116" Height="230">
            <Calendar.BlackoutDates>
                <CalendarDateRange Start="10.13.2013" End="10.19.2013" />
                <CalendarDateRange Start="10.27.2013" End="10.31.2013" />
            </Calendar.BlackoutDates>
        </Calendar>
        <TextBlock HorizontalAlignment="Center" Margin="0,10,0,0" TextWrapping="Wrap" Text="Calender" VerticalAlignment="Top" FontWeight="Bold"/>
        <Button x:Name="SaveWeekBtn" Content="Save Week" HorizontalAlignment="Left" Margin="210,311,0,0" VerticalAlignment="Top" Height="26" Width="161" Click="SaveWeekClick" RenderTransformOrigin="0.5,0.5">
           
        </Button>
    </Grid>
</UserControl>
